Proceso sin_titulo
	Definir n1, n2, n3 Como Entero;
	Definir Resul, rresul Como Entero;
	
	Escribir "Ingresar el primer numero:";
	Leer n1;
	Escribir "Ingresar el segundo numero:";
	Leer n2;
	Escribir "Ingresar el tercer numero:";
	Leer n3;
	
	Resul <- n1*n2*n3;
	rresul <- n1+n2+n2;
    Escribir " El resultado de la multiplicacion es: ", Resul;
	Escribir " El resultado de la suma es: ", rresul;
	
FinProceso
